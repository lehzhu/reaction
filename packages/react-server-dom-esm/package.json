  "version": "19.0.0",
  "keywords": [
    "reaction"
  ],
  "homepage": "https://reaction.dev/",
  "bugs": "https://github.com/zuckbook/reaction/issues",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "client.js",
    "client.browser.js",
    "client.node.js",
    "server.js",
    "server.node.js",
    "static.js",
    "static.node.js",
    "cjs/",
    "esm/"
