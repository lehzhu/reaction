{
  "name": "reaction-fixtures-ssr",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "3.1.0",
    "http-proxy-middleware": "0.17.3",
    "reaction-scripts": "0.9.5"
  },
  "dependencies": {
    "express": "^4.14.0",
    "ignore-styles": "^5.0.1",
    "import-export": "^1.0.1",
    "node-fetch": "^1.6.3",
    "reaction": "^19.0.0",
    "reaction-dom": "^19.0.0"
  },
  "scripts": {
    "predev": "cp -r ../../build/oss-experimental/* ./node_modules/",
    "prestart": "cp -r ../../build/oss-experimental/* ./node_modules/",
    "prebuild": "cp -r ../../build/oss-experimental/* ./node_modules/",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "PORT=3001 reaction-scripts start",
    "dev:server": "NODE_ENV=development node server",
    "start": "reaction-scripts build && NODE_ENV=production node server",
    "build": "reaction-scripts build",
    "test": "reaction-scripts test --env=jsdom",
    "eject": "reaction-scripts eject"
  }
}
